							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "DF_Breed_to_PROD",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"breed": {},
									"species": {},
									"PRODbreed": {},
									"PRODbreed2": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
