								"parameters": {},
								"datasetParameters": {
									"breed": {},
									"ProdSpecies": {},
									"ProdBreed": {},
									"sink1": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
					},
					{
						"name": "DF_Stage_to_PROD",
						"type": "ExecuteDataFlow",
						"dependsOn": [
							{
								"activity": "DF_Breed_to_PROD",
								"dependencyConditions": [
