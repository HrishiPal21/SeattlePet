							},
							"traceLevel": "Fine"
						}
					},
					{
						"name": "DF_Stage_to_PROD",
						"type": "ExecuteDataFlow",
						"dependsOn": [
							{
								"activity": "DF_Breed_to_PROD",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
